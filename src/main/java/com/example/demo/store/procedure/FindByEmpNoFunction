CREATE OR REPLACE FUNCTION FX_GET_EMP_DETAILS 
(
  ENO IN NUMBER 
, NAME OUT VARCHAR2 
, DESIG OUT VARCHAR2 
, SALARY OUT NUMBER
) RETURN NUMBER
AS 
    DNO NUMBER(3);
BEGIN
  SELECT ENAME, JOB,SAL,DETPNO INTO NAME, DESIG, SALARY,DNO FROM EMP WHERE emp_no = ENO;
  
  RETURN dno;
  
END FX_GET_EMP_DETAILS;